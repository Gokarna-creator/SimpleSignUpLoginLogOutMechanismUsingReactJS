] = useState(localStorage.getItem('id'))\n\n    const logOut = ()=>{\n        localStorage.setItem('valid', 'false')\n    }\n\n    return(\n        <div>\n            {\n                (\n                    ()=>{\n                        if(valid){\n                            return (\n                                <div>\n                                    <div>\n                                        <Link to = '/LogOut'>\n                                            <button onClick={logOut}>Logout</button>\n                                        </Link>\n                                    </div>\n                                \n                                   <h1>Login Successful !</h1> \n                                   \n                                   <p> \n                                   <Link to = '/Information'>\n                                        Click here\n                                   </Link> to see your information.</p> \n                                </div>\n                            )\n                        }\n                        else{\n                            return (\n                                <div>\n                                    <h1>Login Failed, Please check your credentials and try again</h1>\n                                </div>\n                            )\n                        }\n                    }\n                )()\n            }\n\n            <hr />\n            <br />\n            <div className='back' >\n                <Link to='/'>\n                    <h2>Back</h2>\n                </Link>\n            </div>\n            \n        </div>\n    )\n\n}\n\nexport default LoggedIn;","/home/gokarna/Every_Programming/projects/facebook/src/js/LogOut.js",[],"/home/gokarna/Every_Programming/projects/facebook/src/js/Information.js",["67","68"],"import React, { useState } from 'react'\n\n\nfunction Information(){\n\n    const getDetails = () => {\n        var c = JSON.parse(loc